{
  "name": "ng2-permission",
  "version": "0.8.2",
  "scripts": {
    "build": "ng build ng2-permission",
    "build:demo": "ng build demo --base-href /ng2-permission/docs/demo/ -c production",
    "start": "npm run build && ng serve -o demo",
    "docs": "npm run docs:build",
    "docs:build": "compodoc -p projects/ng2-permission/tsconfig.doc.json -n ng2-permission -d dist/docs --hideGenerator",
    "docs:serve": "npm run docs:build -- -s",
    "docs:watch": "npm run docs:build -- -s -w",
    "lint": "ng lint --type-check",
    "test": "ng test ng2-permission",
    "release": "standard-version -a",
    "deploy": "npm run build && npm publish dist --registry=https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/e-cloud/ng2-permission"
  },
  "author": {
    "name": "e-cloud",
    "email": "saintscott119@gmail.com"
  },
  "keywords": [
    "angular",
    "ng2-permission"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/e-cloud/ng2-permission/issues"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.6",
    "@angular-devkit/build-ng-packagr": "~0.6.6",
    "@angular/cli": "^6.0.0",
    "@angular/common": "^6.0.0",
    "@angular/compiler": "^6.0.0",
    "@angular/compiler-cli": "^6.0.0",
    "@angular/core": "^6.0.0",
    "@angular/platform-browser": "^6.0.0",
    "@angular/platform-browser-dynamic": "^6.0.0",
    "@angular/router": "^6.0.0",
    "@compodoc/compodoc": "^1.0.4",
    "@types/jasmine": "^2.6.3",
    "@types/lodash": "^4.14.63",
    "@types/lodash-es": "^4.14.5",
    "@types/node": "^9.0.0",
    "codelyzer": "^4.3.0",
    "core-js": "^2.4.1",
    "del": "^3.0.0",
    "gulp": "^3.9.1",
    "gulp-rollup": "^2.11.0",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.0",
    "karma": "^2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^2.0.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "lodash-es": "^4.17.4",
    "protractor": "~5.1.0",
    "rxjs": "^6.0.0",
    "standard-version": "^4.1.0",
    "ts-node": "^6.0.0",
    "tslint": "^5.10.0",
    "tslint-consistent-codestyle": "^1.13.0",
    "typescript": "~2.7.0",
    "zone.js": "^0.8.4"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "standard-version": {
    "scripts": {
      "postbump": "node tools/bump-version.js && git add ."
    }
  },
  "dependencies": {
    "ng-packagr": "^3.0.0"
  }
}
