{
  "name": "ng2-permission",
  "version": "0.6.4",
  "scripts": {
    "build": "gulp build",
    "build:watch": "gulp",
    "docs": "npm run docs:build",
    "docs:build": "compodoc -p tsconfig.json -n ng2-permission -d docs --hideGenerator",
    "docs:serve": "npm run docs:build -- -s",
    "docs:watch": "npm run docs:build -- -s -w",
    "lint": "ng lint",
    "test": "ng test",
    "release": "standard-version -a",
    "deploy": "npm run build && npm publish dist --registry=https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/e-cloud/ng2-permission"
  },
  "author": {
    "name": "e-cloud",
    "email": "saintscott119@gmail.com"
  },
  "keywords": [
    "angular",
    "ng2-permission"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/e-cloud/ng2-permission/issues"
  },
  "dependencies": {
    "@angular/common": "^4.0.0",
    "@angular/core": "^4.0.0",
    "@angular/router": "^4.1.0",
    "@types/lodash-es": "^4.14.5",
    "lodash-es": "^4.17.4"
  },
  "devDependencies": {
    "@angular/cli": "^1.4.1",
    "@angular/compiler": "^4.0.0",
    "@angular/compiler-cli": "^4.0.0",
    "@angular/platform-browser": "^4.0.0",
    "@angular/platform-browser-dynamic": "^4.0.0",
    "@compodoc/compodoc": "^1.0.0-beta.13",
    "@types/jasmine": "2.5.38",
    "@types/lodash": "^4.14.63",
    "@types/node": "~8.0.17",
    "codelyzer": "~3.1.0",
    "core-js": "^2.4.1",
    "del": "^3.0.0",
    "gulp": "^3.9.1",
    "gulp-rollup": "^2.11.0",
    "jasmine-core": "~2.7.0",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.3.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "node-watch": "^0.5.2",
    "protractor": "~5.1.0",
    "rollup": "^0.45.0",
    "run-sequence": "^2.1.0",
    "rxjs": "^5.5.0",
    "standard-version": "^4.1.0",
    "ts-node": "~3.3.0",
    "tslint": "~5.5.0",
    "typescript": "~2.4.0",
    "zone.js": "^0.8.4"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "standard-version": {
    "scripts": {
      "postbump": "node tools/bump-version.js && git add ."
    }
  }
}
